<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="850.0" style="-fx-background-color: #DEF2F1;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.SceneController">

   <AnchorPane layoutX="-2.0" prefHeight="709.0" prefWidth="236.0" style="-fx-background-color: #3AAFA9;">

      <ImageView fitHeight="137.0" fitWidth="200.0" layoutX="21.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="10.0" layoutY="140.0" mnemonicParsing="false" onAction="#createScene" prefHeight="63.0" prefWidth="216.0" style="-fx-background-color: #DEF2F1;" text="Create" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Button>
      <Button layoutX="10.0" layoutY="303.0" mnemonicParsing="false" onAction="#updateDetailsScene" prefHeight="63.0" prefWidth="216.0" style="-fx-background-color: #DEF2F1;" text="Update Details" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Button>
      <Button layoutX="10.0" layoutY="225.0" mnemonicParsing="false" onAction="#showUploadedFiles" prefHeight="63.0" prefWidth="216.0" style="-fx-background-color: #DEF2F1;" text="View Uploaded Files" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Button>
      <Button layoutX="10.0" layoutY="578.0" mnemonicParsing="false" onAction="#logout" prefHeight="63.0" prefWidth="216.0" style="-fx-background-color: #DEF2F1;" text="Log-Out" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Button>
      <Button layoutX="10.0" layoutY="476.0" mnemonicParsing="false" onAction="#terminalScence" prefHeight="63.0" prefWidth="216.0" style="-fx-background-color: #DEF2F1;" text="Terminal" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Button>
      <Button layoutX="10.0" layoutY="388.0" mnemonicParsing="false" onAction="#recover" prefHeight="63.0" prefWidth="216.0" style="-fx-background-color: #DEF2F1;" text="Recover Files" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Button>
   </AnchorPane>
   <Label layoutX="454.0" layoutY="27.0" prefHeight="121.0" prefWidth="200.0" text="Create Files">

      <font>
         <Font name="System Bold" size="35.0" />
      </font>
   </Label>
   <Label layoutX="380.0" layoutY="180.0" text="File Name">
      <font>
         <Font name="System Bold" size="16.0" />
      </font>
   </Label>
   <TextField fx:id="fileNameTextField" layoutX="480.0" layoutY="180.0" prefHeight="25.0" prefWidth="200.0" />
   <Button fx:id="uploadButton" layoutX="490.0" layoutY="230.0" mnemonicParsing="false" onAction="#uploadFile" prefHeight="30.0" prefWidth="150.0" style="-fx-background-color: #3AAFA9;" text="Upload File" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="14.0" />
      </font>
   </Button>

</AnchorPane>